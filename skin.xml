<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml" xmlns="http://www.w3.org/1999/xhtml">

<a:pdf name="pdf">
    <a:style><![CDATA[
        .pdf embed {
            position: relative;
            width: 100%;
            height: 100%;
        }
    ]]></a:style>
    <a:presentation>
        <a:main container="." pdf="./embed">
            <div class="pdf">
                <embed></embed>
            </div>
        </a:main>
    </a:presentation>
</a:pdf>

<a:logentry name="logentry">
    <a:style><![CDATA[
        .log_entry {
            border: 1px solid black;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            margin: 5px;
        }
        
        .error {
            border-color: #f88;
            background-color: #fdd;
        }
        
        .warning {
            border-color: #fa6;
            background-color: #fff0dd;
        }
        
        .summary_line {
            padding: 3px 6px;
            position: relative;
        }

        .summary_line .toggle {
            width: 10px;
            height: 10px;
            background-image: url("images/tree_close_arrow.png");
            background-position: -10px 0;
            position: absolute;
            top: 8px;
            left: 6px;
            display: none;
        }

        .summary_line .message {
            line-height: 20px;
        }
        
        .content {
            padding: 6px;
            padding-top: 2px;
            font-size: 0.9em;
            color: #666;
            white-space: pre;
            display: none;
        }

        .hasContent .summary_line .toggle {
            display: block;
        }
        .hasContent .summary_line .message {
            margin-left: 15px;
        }

        .hideContent .content {
            display: none;
        }
        .hideContent .summary_line .toggle {
            background-position: 0px 0px;
        }

        .showContent .content {
            display: block;
        }
        .showContent .summary_line .toggle {
            background-position: -10px 0px;
        }

    ]]></a:style>
    <a:presentation>
        <a:main 
          container = "."
          toggle    = "./div[1]/div[1]"
          summary   = "./div[1]/div[2]"
          goto      = "./div[1]/div[2]"
          content   = "./div[2]">
            <div class="log_entry">
                <div class="summary_line">
                    <div class="toggle"></div>
                    <div class="message"></div>
                    <div class="goto"></div>
                </div>
                <div class="content"></div>
            </div>
        </a:main>
    </a:presentation>
</a:logentry>

<a:tab name="pdf_tab">
    <a:style><![CDATA[
    .pdf_tab {
        padding : 0 0 0 0;
        margin : 0 auto;
        overflow: hidden;
        position:relative;
        padding-top: 32px;
        border-bottom: 1px solid white;
        background-color:#e8e8e8;
    }

    .pdf_tab .btnsesssioncontainer {
        padding : 5px 20px 0 1px;
        z-index : 100;
        height: 25px;
        overflow: visible;
        white-space: nowrap;
        color: #f5f5f5;
        font-family : Arial;
        font-size : 12px;
        border-bottom : 1px solid white;
        background: -webkit-gradient(linear,left bottom,left top,color-stop(0, #262829),color-stop(1, #2f3031));
        background: -moz-linear-gradient(center bottom,#262829 0%, #2f3031 100%);
        margin-top : -32px;
        width : auto;
        
        -moz-box-shadow    : 0px 1px 0px rgba(255, 255, 255, 0.05) inset;
        -webkit-box-shadow : 0px 1px 0px rgba(255, 255, 255, 0.05) inset;
        box-shadow         : 0px 1px 0px rgba(255, 255, 255, 0.05) inset;
    }

    .pdf_tab .btnsesssioncontainer>div {
        cursor: default;
        display: block;
        display : inline-block;
        margin: 0 0 0 0;
        padding: 0;
        position : relative;
        vertical-align : bottom;
    }

    .pdf_tab .btnsesssioncontainer .session_btn {
        padding      : 0 20px;
        position     : relative;
        margin-right : -20px;
        margin-top   : 0;
        height:25px;
        border-bottom:1px solid white;
        float: right;
    }

    .pdf_tab .btnsesssioncontainer .session_btn .tab_middle {
        background-image    : url(images/editor_tab.png);
        background-position : 0 -104px;
        height              : 20px;
        position            : relative;
        overflow            : hidden;
        padding             : 5px 0 0 0;
    }

    .pdf_tab .btnsesssioncontainer .session_btn .tab_middle .sessiontab_icon {
        height:8px;
        left:0;
        position:absolute;
        top:10px;
        width:5px;
    }
    
    .pdf_tab .btnsesssioncontainer .session_btn .sessiontab_icon {
        background-repeat: no-repeat;
        background-position: 0 0;
    }

    .pdf_tab .btnsesssioncontainer .session_btn .tab_middle .sessiontab_title {
        overflow : hidden;
        padding  : 0 0 0 0;
        -moz-user-select: -moz-none;
        -khtml-user-select: none;
        -webkit-user-select: none;
        -o-user-select: none;
        user-select: none;
    }

    .pdf_tab .btnsesssioncontainer .session_btn.btnclose .tab_middle .sessiontab_title {
        margin-right : 15px;
        margin-left : 8px;
    }

    .pdf_tab .scale .session_btn .sessiontab_title{
        display : block;
        overflow : hidden;
        text-overflow : ellipsis;
        white-space : nowrap;
    }

    .pdf_tab .btnsesssioncontainer .tab_left,
    .pdf_tab .btnsesssioncontainer .tab_right {
        position            : absolute;
        height              : 25px;
        width               : 20px;
        background-image    : url(images/editor_tab.png);
        background-repeat   : no-repeat;
        top                 : 0;
    }

    .pdf_tab .btnsesssioncontainer .tab_left {
        background-position : 0 -78px;
        left                : 0;
    }
    .pdf_tab .btnsesssioncontainer .tab_right {
        background-position : 0 -130px;
        right               : 0;
    }

    .pdf_tab .btnsesssioncontainer div.down {
        text-shadow: none;
        color: #d9d9d9;
    }

    .pdf_tab .btnsesssioncontainer div.over .tab_middle {
        background-position : 0 -182px;
    }
    .pdf_tab .btnsesssioncontainer div.over .tab_left {
        background-position : 0 -156px;
    }
    .pdf_tab .btnsesssioncontainer div.over .tab_right {
        background-position : 0 -208px;
    }

    .pdf_tab .btnsesssioncontainer div.curbtn {
        color       : #333333;
        cursor      : default;
        text-shadow : rgba(255, 255, 255, 1) 0px 1px 0px;
        z-index     : 1;
        height:25px;
        border-bottom:1px solid #e8e8e8;
    }

    .pdf_tab .btnsesssioncontainer div.curbtn .tab_middle {
        background-position : 0 -26px;
        padding             : 6px 0 0 0;
        height:19px;
        color:#000000;
    }
    .pdf_tab .btnsesssioncontainer div.curbtn .tab_left {
        background-position : 0 0;
    }
    .pdf_tab .btnsesssioncontainer div.curbtn .tab_right {
        background-position : 0 -52px;
    }

    .pdf_tab .scale .btn .tab_middle {
        display : block;
        overflow : hidden;
        text-overflow : ellipsis;
        white-space : nowrap;
    }

    .pdf_tabFocus .curbtn{
    }

    .pdf_tab .btnsesssioncontainer .btnclose strong {
        width               : 14px;
        height              : 14px;
        display             : block;
        position            : absolute;
        right               : 0;
        top                 : 7px;
        background-image    : url(images/close_tab_btn.png);
        background-repeat   : no-repeat;
        background-position : 0 -14px;
    }

    .pdf_tab .btnsesssioncontainer .btnclose strong:hover {
        background-position : -14px -14px;
    }

    .pdf_tab .btnsesssioncontainer .btnclose strong:active {
        background-position : -28px -14px;
    }

    .pdf_tab .btnsesssioncontainer .curbtn.btnclose strong {
        background-position : 0 0;
        top: 7px;
    }

    .pdf_tab .btnsesssioncontainer .curbtn.btnclose strong:hover {
        background-position : -14px 0;
    }

    .pdf_tab .btnsesssioncontainer .curbtn.btnclose strong:active {
        background-position : -28px 0;
    }

    .pdf_tab .editor_bg {
        top : 32px;
        bottom : 0px;
        height : auto;
        position : absolute;
        margin: 0 auto;
        background: #e8e8e8 url(images/editor_tab_bg.png) repeat 0 0;
        /*border-bottom: 1px solid #8c8c8c;
        border-left: 1px solid #8c8c8c;
        border-right: 1px solid #8c8c8c;*/
        overflow: hidden;
        clear: both;
        left: 0;
        right: 0;
    }

    .pdf_tab .session_page {
        display : none;
        position : absolute;
        top : 32px;
        bottom : 0;
        left : 0;
        right : 0;
        z-index : 10;
        margin: 0 auto;
        background-color: #e8e8e8;
        /*border-bottom: 1px solid #8C8C8C;*/
        border-top:3px solid #e8e8e8;
        overflow: hidden;
        clear: both;
    }

    .pdf_tab .session_page.curpage{
        display : block;
    }
]]></a:style>
<a:presentation>
    <a:main pages="." buttons="div[1]">
        <div class="pdf_tab">
            <div class="btnsesssioncontainer"> </div>
            <div class="editor_bg">
            </div>
        </div>
    </a:main>
    <a:button
      caption   = "div[1]/div[2]/text()"
      container = "div"
      icon      = "div[1]/div"
      maxwidth  = "150"
      minwidth  = "0">
        <div class="session_btn">
            <div class="tab_middle">
                <div class="sessiontab_icon"> </div>
                <div class="sessiontab_title">-</div>
            </div>
            <div class="tab_left"> </div>
            <div class="tab_right"> </div>
        </div>
    </a:button>
    <a:btnclose>
        <strong> </strong>
    </a:btnclose>
    <a:page container=".">
        <div class="session_page"> </div>
    </a:page>
</a:presentation>
</a:tab>

</a:skin>
